    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "lucide-react": "^0.244.0",
    "next": "14.1.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-aria-components": "1.0.0-alpha.6",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "surrealdb.js": "^0.8.4",
    "swr": "^2.1.5",
    "tailwind-merge": "^1.13.1",
