{
  "name": "surrealdb-stickies",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names surreal,next \"pnpm run dev:surreal\" \"pnpm run dev:next\"",
    "dev:surreal": "surreal start --allow-all --auth --user root --pass root --bind 0.0.0.0:3001 file:dev.db",
